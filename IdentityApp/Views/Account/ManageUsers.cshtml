@model IEnumerable<IdentityApp.Models.ApplicationUser>

@{
    ViewBag.Title = "ManageUsers";
}

<h2>ManageUsers</h2>

<p>
    @Html.ActionLink("Add new user", "AddNewUser")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @if (item.UserName != "admin@yandex.ru")
            {@Html.ActionLink("Delete", "Delete", new {id = item.Id})}
        </td>
    </tr>
}

</table>
